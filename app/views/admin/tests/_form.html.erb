<h2><%= @test.title %></h2>

<div class="card card border-primary col-md-6">
  <div class="card-body">
    <%= form_with model: [:admin, @test] do |form| %>
      <%= render 'shared/errors', resource: @test %>
      <div class="form-group">
        <p class="col-md-4">
          <%= form.label :test_title %><br>
          <%= form.text_field :title, class: 'form-control' %>
        </p>
      </div>
    <p> <%= form.submit class: "btn btn-outline-primary mt-2" %> </p>

    <% end %>
  </div>
</div>