<tr>
  <td ><%= test.title %></td>
  <td style="text-align: center;"><%= test_level(test)%></td>
  <td style="text-align: center;"><%= test.questions.count %></td>
  <td style="text-align: center;"><%= link_to t('.show'), admin_test_path(test) %></td>
  <td style="text-align: center;"><%= link_to t('.edit'), edit_admin_test_path(test) %></td>
  <td style="text-align: center;"><%= button_to t('.destroy'), admin_test_path(test),
                            method: :delete,
                            data: { confirm: 'Are you sure?' } %>
    </td>
</tr>
